{
  "crons": [
    {
      "path": "/api/cron/content",
      "schedule": "0 */4 * * *"
    }
  ],
  "functions": {
    "src/app/api/content/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/news/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/rss/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/newsletter/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/automated-content/route.ts": {
      "memory": 1024,
      "maxDuration": 30
    },
    "src/app/api/search/route.ts": {
      "memory": 512,
      "maxDuration": 15
    },
    "src/app/api/cron/content/route.ts": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=300, stale-while-revalidate"
        }
      ]
    }
  ]
}
